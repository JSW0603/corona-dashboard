    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv(\"data/time_confirmed.csv\")\n",
    "df = df.drop([\"Province/State\", \"Country/Region\", \"Lat\",\"Long\"], axis=1).sum().reset_index(name=\"total\")\n",
    "df = df.rename(columns={\"index\": \"date\"})\n",
    "df"
